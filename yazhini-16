# Enter your code here. Read input from STDIN. Print output to STDOUT

a=int(input())
if(a<10 or a>99):
    print("Invalid Input")
else:
   d1=a//10  

    d2=a%10   
    digit_sum=d1+d2
    print(f"Sum of Digit {a} is {str(digit_sum).zfill(3)}")
